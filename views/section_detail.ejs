    </body>
    <script>
        var rotation = function (ele){
           $(ele).rotate({
              angle:0, 
              animateTo:360, 
              callback: rotation,
              easing: function (x,t,b,c,d){        // t: current time, b: begInnIng value, c: change In value, d: duration
                  return c*(t/d)+b;
              }
           });
        }

        $(function(){
            $('#cutoff').change( function(){
                var x = $('#cutoff').val();
                if(isNaN(x) || !isFinite(x)) $('#cutoff').val("");
            });
            $('#hours').change( function(){
                var x = $('#hours').val();
                if(isNaN(x) || !isFinite(x)){
                  $('#hours').val("");  
                }else{
                    x = parseInt(x);
                    if( x < 0 || x >23){
                      $('#hours').val("");  
                    }
                }
            });
            $('#minutes').change( function(){
                var x = $('#minutes').val();
                if( isNaN(x) || !isFinite(x)){
                    $('#minutes').val("");
                }else{
                    x = parseInt(x);
                    if( x<0 || x>59){
                      $('#minutes').val("");  
                    }
                }
            });
            $('#quest').change( function(){
                var x = $('#quest').val();
                if(isNaN(x) || !isFinite(x)) $('#quest').val("");
            });
        });


    </script>
    
</html>